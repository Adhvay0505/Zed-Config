{
  "notification_panel": {
    "dock": "left"
  },
  "chat_panel": {
    "dock": "left"
  },
  "outline_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_family": "JetBrains Mono",
  "buffer_font_family": "JetBrains Mono",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "base_keymap": "VSCode",
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "auto_install_extensions": {
    "dart": true,
    "git-firefly": true,
    "ruff": true,
    "xcode-themes": true,
    "toml": true
  },
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light Soft",
    "dark": "Gruvbox Dark Soft"
  },
  "terminal": {
    "detect_venv": {
      "on": {
        "directories": [".env", "env", ".venv", "venv"],
        "activate_script": "default"
      }
    }
  }
}
